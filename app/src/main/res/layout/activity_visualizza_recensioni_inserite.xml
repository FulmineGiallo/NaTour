<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.PannelloAdmin.visualizza_recensioni_inserite">

    <TextView
        android:id="@+id/txt_val_recensioni_totale"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="val"
        android:textSize="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.975"
        app:layout_constraintStart_toEndOf="@+id/txt_recensioni_totale"
        app:layout_constraintTop_toBottomOf="@+id/txt_val_recensioni_anno" />

    <TextView
        android:id="@+id/txt_val_recensioni_anno"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="val"
        android:textSize="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.97"
        app:layout_constraintStart_toEndOf="@+id/txt_recensioni_anno"
        app:layout_constraintTop_toBottomOf="@+id/txt_val_recensioni_mese" />

    <TextView
        android:id="@+id/txt_val_recensioni_mese"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="val"
        android:textSize="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.966"
        app:layout_constraintStart_toEndOf="@+id/txt_recensioni_mese"
        app:layout_constraintTop_toBottomOf="@+id/txt_val_recensioni_settimana" />

    <TextView
        android:id="@+id/txt_val_recensioni_settimana"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="val"
        android:textSize="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.946"
        app:layout_constraintStart_toEndOf="@+id/txt_recensioni_settimana"
        app:layout_constraintTop_toBottomOf="@+id/txt_val_recensioni_giorno" />

    <ImageButton
        android:id="@+id/btn_indietro_recensioni_inserite"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00FFFFFF"
        android:minWidth="48dp"
        android:minHeight="48dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.044"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023"
        app:srcCompat="@drawable/ic_icon_undo"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/txt_recensioni_mese"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="12dp"
        android:text="Numero di recensioni questo mese :"
        android:textSize="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_recensioni_settimana" />

    <TextView
        android:id="@+id/txt_recensioni_settimana"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="12dp"
        android:text="Numero di recensioni questa settimana :"
        android:textSize="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_recensioni_giorno" />

    <TextView
        android:id="@+id/txt_recensioni_anno"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="12dp"
        android:text="Numero di recensioni quest'anno :"
        android:textSize="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_recensioni_mese" />

    <TextView
        android:id="@+id/txt_recensioni_totale"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="12dp"
        android:text="Numero di recensioni in totale :"
        android:textSize="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_recensioni_anno" />

    <TextView
        android:id="@+id/txt_recensioni_descrizione"
        android:layout_width="293dp"
        android:layout_height="68dp"
        android:layout_marginTop="21dp"
        android:text="Visualizza Recensioni inserite"
        android:textColor="@color/black"
        android:textSize="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.703"
        app:layout_constraintStart_toEndOf="@+id/btn_indietro_recensioni_inserite"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_recensioni_giorno"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="36dp"
        android:text="Numero di recensioni oggi :"
        android:textSize="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_indietro_recensioni_inserite" />

    <TextView
        android:id="@+id/txt_val_recensioni_giorno"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:text="val"
        android:textSize="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.979"
        app:layout_constraintStart_toEndOf="@+id/txt_recensioni_giorno"
        app:layout_constraintTop_toBottomOf="@+id/btn_indietro_recensioni_inserite" />

</androidx.constraintlayout.widget.ConstraintLayout>