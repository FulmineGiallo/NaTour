<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.admin.VisualizzaStatistiche">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:overScrollMode="never"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <TextView
                        android:id="@+id/txt_statistiche_descrizione4"
                        android:layout_width="match_parent"
                        android:layout_height="34dp"
                        android:layout_marginTop="10dp"
                        android:gravity="center"
                        android:text="Ricerche effettuate"
                        android:textColor="@color/black"
                        android:textSize="24dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/txt_val_accessi_totale" />

                    <TextView
                        android:id="@+id/txt_val_foto_totale"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_marginTop="10dp"
                        android:background="#f5f3ed"
                        android:gravity="center"
                        android:paddingEnd="8dp"
                        android:text="val"
                        android:textSize="30dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/txt_statistiche_descrizione7" />

                    <TextView
                        android:id="@+id/txt_val_recensioni_totale"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_marginTop="10dp"
                        android:background="#f5f3ed"
                        android:gravity="center"
                        android:paddingEnd="8dp"
                        android:text="val"
                        android:textSize="30dp"
                        app:layout_constraintTop_toBottomOf="@+id/txt_statistiche_descrizione5"
                        tools:layout_editor_absoluteX="247dp" />

                    <TextView
                        android:id="@+id/txt_val_accessi_totale"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_marginTop="10dp"
                        android:background="#f5f3ed"
                        android:gravity="center"
                        android:paddingEnd="8dp"
                        android:text="val"
                        android:textSize="30dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/txt_statistiche_descrizione3" />

                    <TextView
                        android:id="@+id/txt_statistiche_descrizione7"
                        android:layout_width="match_parent"
                        android:layout_height="34dp"
                        android:layout_marginTop="10dp"
                        android:gravity="center"
                        android:text="Foto caricate "
                        android:textColor="@color/black"
                        android:textSize="24dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/txt_val_segnalazioni_totale2" />

                    <TextView
                        android:id="@+id/txt_val_segnalazioni_totale2"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_marginTop="10dp"
                        android:background="#f5f3ed"
                        android:gravity="center"
                        android:paddingEnd="8dp"
                        android:text="val"
                        android:textSize="30dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/txt_statistiche_descrizione6" />

                    <TextView
                        android:id="@+id/txt_val_itinerari_totale"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_marginTop="10dp"
                        android:background="#f5f3ed"
                        android:gravity="center"
                        android:paddingEnd="8dp"
                        android:text="val"
                        android:textSize="30dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/txt_statistiche_descrizione" />

                    <TextView
                        android:id="@+id/txt_statistiche_descrizione6"
                        android:layout_width="match_parent"
                        android:layout_height="34dp"
                        android:layout_marginTop="10dp"
                        android:gravity="center"
                        android:text="Segnalazioni effettuate"
                        android:textColor="@color/black"
                        android:textSize="24dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/txt_val_recensioni_totale" />

                    <TextView
                        android:id="@+id/txt_statistiche_descrizione3"
                        android:layout_width="match_parent"
                        android:layout_height="34dp"
                        android:layout_marginTop="10dp"
                        android:gravity="center"
                        android:text="Accessi effettuati"
                        android:textColor="@color/black"
                        android:textSize="24dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/txt_val_registrati_totale" />

                    <TextView
                        android:id="@+id/txt_statistiche_descrizione2"
                        android:layout_width="match_parent"
                        android:layout_height="34dp"
                        android:layout_marginTop="10dp"
                        android:gravity="center"
                        android:text="Utenti registrati"
                        android:textColor="@color/black"
                        android:textSize="24dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/txt_val_itinerari_totale" />

                    <TextView
                        android:id="@+id/txt_val_ricerche_totale"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_marginTop="10dp"
                        android:background="#f5f3ed"
                        android:gravity="center"
                        android:paddingEnd="8dp"
                        android:text="val"
                        android:textSize="30dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/txt_statistiche_descrizione4" />

                    <TextView
                        android:id="@+id/txt_val_registrati_totale"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_marginTop="10dp"
                        android:background="#f5f3ed"
                        android:gravity="center"
                        android:paddingEnd="8dp"
                        android:text="val"
                        android:textSize="30dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/txt_statistiche_descrizione2" />

                    <TextView
                        android:id="@+id/txt_statistiche_descrizione5"
                        android:layout_width="match_parent"
                        android:layout_height="34dp"
                        android:layout_marginTop="10dp"
                        android:gravity="center"
                        android:text="Recensioni effettuate"
                        android:textColor="@color/black"
                        android:textSize="24dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/txt_val_ricerche_totale" />

                    <TextView
                        android:id="@+id/txt_statistiche_descrizione"
                        android:layout_width="match_parent"
                        android:layout_height="34dp"
                        android:gravity="center"
                        android:text="Itinerari inseriti"
                        android:textColor="@color/black"
                        android:textSize="24dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                </androidx.constraintlayout.widget.ConstraintLayout>

            </LinearLayout>
        </ScrollView>
    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>